{
  "name": "@bi-fraction/fraction",
  "description": "Fraction library",
  "version": "0.0.1",
  "keywords": [
    "Fraction",
    "BigInt"
  ],
  "author": "Logan Luo <github.com/logan272>",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "tsc;tsc --module commonjs --outDir dist/cjs",
    "watch": "chokidar 'src/**/*.ts' -c 'pnpm -wc exec turbo run build --filter=detect-provider'",
    "typedoc": "typedoc src/index.ts --out ../../docs/api/fraction",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "ts-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fraction-asset/fraction-asset.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/fraction-asset/fraction-asset/issues"
  },
  "homepage": "https://github.com/fraction-asset/fraction-asset#readme",
  "dependencies": {
    "bignumber.js": "^9.1.2"
  }
}
